{
  "name": "egg-gridfs",
  "version": "1.0.0",
  "description": "egg plugin for MongoDB GridFS.",
  "eggPlugin": {
    "name": "gridfs"
  },
  "keywords": [
    "egg",
    "eggPlugin",
    "egg-plugin",
    "GridFs",
    "egg-gridfs"
  ],
  "dependencies": {
    "mongodb": "^3.5.6"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg": "^2.16.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "npm run lint -- --fix && egg-bin pkgfiles && npm run test-local",
